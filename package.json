{
    "name": "vision-server",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --watch --debug 0.0.0.0:9229",
        "prestart:prod": "rimraf dist && tsc && cp -r ./src/assets ./dist",
        "start:prod": "node dist/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:unit": "jest --config ./src/test/unit/jest-unit.json",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage --runInBand --forceExit",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./src/test/e2e/jest-e2e.json --forceExit --runInBand",
        "test:ci": "jest --config ./src/test/e2e/jest-e2e.json --forceExit --runInBand",
        "test:dev": "jest --config ./src/test/e2e/jest-e2e.json --forceExit --runInBand",
        "test:prov": "jest --config ./src/test/providers/jest-providers.json --coverage --forceExit --runInBand",
        "webpack": "webpack --config webpack.config.js",
        "check-dep": "../../exploitation/dependency-check/bin/dependency-check.sh --project \"Gateway\" --format XML --format HTML --scan .",
        "sonar": "npm run check-dep && sonar-scanner",
        "openapi": "export JWT_SECRET=dummy && npx ts-node ./src/generate-openapi.ts"
    },
    "dependencies": {
        "@algotech-ce/core": "3.1.0",
        "@algotech-ce/interpretor": "3.1.0",
        "@nestjs/axios": "2.0.0",
        "@nestjs/bull": "0.6.3",
        "@nestjs/common": "9.2.1",
        "@nestjs/core": "9.2.1",
        "@nestjs/microservices": "9.2.1",
        "@nestjs/mongoose": "9.2.1",
        "@nestjs/platform-express": "9.2.1",
        "@nestjs/platform-socket.io": "9.2.1",
        "@nestjs/platform-ws": "9.2.1",
        "@nestjs/swagger": "6.1.4",
        "@nestjs/websockets": "9.2.1",
        "@ngx-translate/core": "13.0.0",
        "@ngx-translate/http-loader": "6.0.0",
        "agenda": "4.2.1",
        "angular2-uuid": "1.1.1",
        "async": "3.2.4",
        "bull": "4.10.4",
        "bullmq": "3.12.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "crypto-js": "3.2.1",
        "cryptr": "4.0.2",
        "engine.io": "6.4.2",
        "image-size": "1.0.0",
        "json2csv": "5.0.3",
        "jsonwebtoken": "9.0.2",
        "jszip": "3.10.1",
        "lodash": "4.17.21",
        "moment": "2.29.4",
        "mongo-gridfs": "1.1.0",
        "mongodb": "4.17.0",
        "mongoose": "6.12.0",
        "mssql": "9.3.0",
        "mysql2": "3.6.0",
        "nats": "2.10.2",
        "node-firebird": "0.9.9",
        "nodemailer": "6.9.4",
        "node-gcm": "1.0.4",
        "oracledb": "5.1.0",
        "pg": "8.6.0",
        "queueing-subject": "0.3.4",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.8.0",
        "sharp": "0.31.3",
        "swagger-ui-express": "4.3.0",
        "uniqid": "5.1.0",
        "tough-cookie": "^4.1.3",
        "degenerator": "5.0.1"
    },
    "overrides": {
        "degenerator": "$degenerator",
        "tough-cookie": "$tough-cookie",
        "engine.io": "$engine.io"
    },
    "devDependencies": {
        "@nestjs/cli": "9.5.0",
        "@nestjs/schematics": "9.0.4",
        "@nestjs/testing": "9.2.1",
        "@types/chai": "4.3.0",
        "@types/express": "4.17.8",
        "@types/jest": "26.0.15",
        "@types/js-yaml": "4.0.5",
        "@types/node": "18.13.0",
        "@types/superagent": "4.1.10",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "4.1.3",
        "@typescript-eslint/eslint-plugin": "4.6.1",
        "@typescript-eslint/parser": "4.6.1",
        "axios": "1.4.0",
        "chai": "4.3.9",
        "eslint": "7.12.1",
        "eslint-config-prettier": "7.2.0",
        "eslint-plugin-prettier": "3.1.4",
        "hot-formula-parser": "3.0.2",
        "jest": "29.3.1",
        "jest-jasmine2": "^29.3.1",
        "jest-junit": "15.0.0",
        "js-yaml": "4.1.0",
        "prettier": "2.1.2",
        "supertest": "6.0.0",
        "ts-jest": "29.0.3",
        "ts-loader": "9.4.2",
        "ts-node": "9.0.0",
        "tsconfig-paths": "3.9.0",
        "tslint": "5.11.0",
        "typescript": "4.6.3",
        "webpack": "5.76.0",
        "webpack-cli": "5.0.1",
        "webpack-node-externals": "1.7.2"
    },
    "jest": {
        "bail": false,
        "verbose": true,
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "coveragePathIgnorePatterns": [
            "database",
            "interfaces",
            "common",
            "test/e2e"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
